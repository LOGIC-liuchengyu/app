.pages{
    flex-direction: column;
    display: flex;
    background-color: rgb(241, 241, 241);
}
.personalInformation{
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
    width: 100%;
    height: 170px;
    background-color: rgb(255, 255, 255);
    margin-bottom: 3px;
}
.picture{
    width: 60px;
    height: 60px;
    border-radius: 50%;
}
.personalName{
    font-size: 14px;
    padding-top: 10px;
}
.item{
    display: flex;
    align-items: center;
    margin-top: 1px;
    width: 100%;
    height: 46px;
    background: white;
}
.itemLeft{
    align-items: center;
    flex-grow: 1;
    padding-left:10px; 
}
.itemRight{
    display: flex;
    flex-direction: row;
    justify-content: flex-end;
    flex-grow: 1;
    padding-right: 10px;

}
.icon{
    width: 24px;
    height: 24px;
}
.textLeft{
    font-size: 16px;
}
.textRight{
    font-size: 14px;
}